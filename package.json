  "scripts": {
    "test": "webpack --config webpack.config.demo.js && node test/testLauncher.js",
    "legacy-test": "webpack --config webpack.config.demo.js && node node_modules/babel-cli/bin/babel.js test/flows --extensions=\".es6\" --out-dir test/flows && node test/phantomFlow.js",
    "build": "webpack --config webpack.config.prod.js",
    "demo": "webpack --config webpack.config.demo.js && node test/server.js start",
    "dev": "webpack-dev-server --config webpack.config.dev.js"
  },
  "repository": {
    "type": "git",
