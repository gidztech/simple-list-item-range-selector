  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.1.1",
    "chromeless": "^1.2.0",
    "css-loader": "^0.28.4",
    "express": "^4.15.4",
    "fs": "0.0.1-security",
    "html-webpack-plugin": "^2.30.1",
    "mocha": "^3.5.0",
